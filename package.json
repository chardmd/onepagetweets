{
  "name": "onepagetweets",
  "version": "1.0.0",
  "description": "Monolith webapp for onepagetweets.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/chardmd/tweets.git"
  },
  "author": "Richard Dimalanta",
  "contributors": [
    "Richard Dimalanta (https://github.com/chardmd)"
  ],
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "start": "node app.js",
    "debug": "nodemon --inspect app.js",
    "dc:up": "docker-compose -f dc-stack-dev.yml build && env $(cat .env | grep ^[A-Z] | xargs) docker stack deploy -c dc-stack-dev.yml app && docker service ls",
    "dc:kill": "docker stack rm app",
    "dc:clean": "docker rm -f $(docker ps -a -q) && docker volume prune -f",
    "dc:clean:img": "docker rmi $(docker images -a -q) -f",
    "dc:cert": "docker-compose -f dc-cert.yml build  && docker-compose -f dc-cert.yml up -d && docker-compose -f dc-cert.yml ps",
    "test": "nyc mocha --timeout=10000 --exit",
    "lint": "eslint \"**/*.js\"",
    "copy": "copyfiles -f ./components/*/client/*.js ./public/js"
  },
  "dependencies": {
    "@sentry/node": "^5.27.3",
    "@sentry/tracing": "^5.27.3",
    "@yaireo/tagify": "^3.21.4",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "font-awesome": "^4.7.0",
    "handlebars-helpers": "^0.10.0",
    "jquery": "^3.5.1",
    "jquery.easing": "^1.4.1",
    "lodash": "^4.17.20",
    "lusca": "^1.6.1",
    "mongoose": "^5.8.9",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^2.0.2",
    "parsleyjs": "^2.9.2",
    "passport": "^0.4.1",
    "passport-twitter": "^1.0.4",
    "popper.js": "^1.16.1",
    "sharer.js": "^0.4.1",
    "turbolinks": "^5.2.0",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "copyfiles": "^2.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "prettier": "1.19.1",
    "sinon": "^9.0.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=10.18.0",
    "npm": ">=6.13.4"
  }
}
